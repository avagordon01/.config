[user]
    email = pgavagordon@gmail.com
    name = Ava Gordon
[alias]
    fa = fetch --all --prune --tags --prune-tags
    drop = reset HEAD --hard
    goa = log --graph --oneline --all
    shad = describe --match --no-match --always --abbrev --dirty
    sha = rev-parse --short HEAD
[push]
    default = current
[fetch]
    prune = true
[diff]
    algorithm = histogram
